class QXRect{
	QXRect(){
    }
	QXRect(int l, int t,int r,int b){
		left = l;
        top = t;
        right = r;
        bottom = b;
    }
    
	int left, top, right, bottom;
    
    QXPoint centerPoint(){
		return new QXPoint((left + right) / 2, (top + bottom) / 2);
    }
    
    int width(){
		return right - left;
    }
    
    int height(){
		return bottom - top;
    }
};